{
  "name": "reki",
  "version": "0.0.0",
  "description": "Finnjamboree 2016 camp registry system",
  "main": "src/server/bootstrap.js",
  "scripts": {
    "start": "node .",
    "start-silent": "node . > /dev/null 2>&1",
    "watch": "nodemon --watch src",
    "set-password": "babel-node scripts/set-password.js",
    "create-user": "babel-node scripts/create-user.js",
    "delete-user": "babel-node scripts/delete-user.js",
    "dev-login": "babel-node scripts/dev-login.js",
    "postinstall": "scripts/deploy-bundle.sh",
    "test": "npm run linter && npm run test-functional && npm run test-e2e",
    "linter": "eslint --ext js --ext jsx src test scripts webpack.development.config.babel.js webpack.production.config.babel.js",
    "test-functional": "NODE_ENV=test ENABLE_OFFLINE_LOGIN=true mocha --timeout 4000 --require babel-core/register --require babel-polyfill test/functional --exit",
    "test-one": "NODE_ENV=test ENABLE_OFFLINE_LOGIN=true mocha --timeout 4000 --require babel-core/register --require babel-polyfill --exit",
    "test-watch": "NODE_ENV=test ENABLE_OFFLINE_LOGIN=true mocha --watch --timeout 4000 --require babel-core/register --require babel-polyfill --exit",
    "pretest-e2e": "NODE_ENV=test npm run seed-database && npm run bundle",
    "test-e2e-wdio": "NODE_ENV=test wdio wdio.conf.js",
    "test-e2e": "NODE_ENV=test PORT=3005 start-test start-silent 3005 test-e2e-wdio",
    "seed-database": "babel-node scripts/seed-database.js",
    "create-fake-data": "babel-node scripts/create-fake-data.js",
    "clear-temporary-tables": "babel-node src/kuksa-integration/clear-temporary-tables.js",
    "fetch-data": "babel-node src/kuksa-integration/fetch-data.js",
    "rebuild-tables": "babel-node src/kuksa-integration/rebuild-tables.js",
    "fetch-from-kuksa": "npm run clear-temporary-tables && npm run fetch-data && npm run rebuild-tables",
    "bundle": "NODE_ENV=production webpack -p --config webpack.production.config.babel.js",
    "bundle-watch": "NODE_ENV=production webpack -p -w --config webpack.production.config.babel.js",
    "mock-kuksa": "babel-node test/utils/kuksa-integration/mock/mock-kuksa.js"
  },
  "dependencies": {
    "alt": "^0.18.2",
    "alt-container": "^1.0.2",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "bluebird": "^3.0.5",
    "body-parser": "^1.18.3",
    "bootstrap-sass": "^3.3.6",
    "commander": "^2.15.1",
    "compression": "^1.7.2",
    "connect-flash": "^0.1.1",
    "cors": "^2.7.1",
    "css-loader": "^0.28.11",
    "errorhandler": "^1.4.2",
    "express-enforces-ssl": "^1.1.0",
    "express-session": "^1.15.6",
    "faker": "^3.0.1",
    "file-loader": "^1.1.11",
    "helmet": "^3.12.1",
    "history": "^2.0.1",
    "http-proxy": "^1.17.0",
    "inquirer": "^1.0.0",
    "js-cookie": "^2.1.1",
    "kuksa-event-api-client": "https://github.com/partio-scout/kuksa-event-api-client/releases/download/v2.3.1/kuksa-event-api-client.tar.gz",
    "lodash": "^4.17.10",
    "loopback": "^2.39.2",
    "loopback-connector-postgresql": "^2.4.0",
    "loopback-datasource-juggler": "2.57.0",
    "moment": "^2.22.2",
    "morgan": "^1.7.0",
    "node-sass": "^4.9.0",
    "passport": "^0.4.0",
    "passport-http": "^0.3.0",
    "passport-saml": "^0.33.0",
    "react": "^0.14.3",
    "react-bootstrap": "^0.28.1",
    "react-dom": "^0.14.3",
    "react-router": "^2.0.0",
    "react-router-bootstrap": "^0.20.1",
    "react-spinner": "^0.2.6",
    "sass-loader": "^7.0.3",
    "sequelize": "^4.38.0",
    "serve-favicon": "^2.5.0",
    "style-loader": "^0.21.0",
    "superagent": "^3.8.3",
    "uglify-js": "^2.6.1",
    "url-loader": "^1.0.1",
    "webpack": "^4.12.2",
    "webpack-dev-server": "^3.1.4"
  },
  "devDependencies": {
    "chai": "^3.4.1",
    "chai-as-promised": "^5.1.0",
    "chai-datetime": "^1.4.1",
    "eslint": "^4.19.1",
    "eslint-plugin-react": "^7.10.0",
    "express": "^4.16.3",
    "mocha": "^5.2.0",
    "nodemon": "^1.17.5",
    "react-hot-loader": "^4.3.3",
    "sinon": "^4.5.0",
    "sinon-express-mock": "^2.0.4",
    "start-server-and-test": "^1.7.11",
    "supertest": "^3.1.0",
    "webdriverio": "^3.2.6",
    "webpack-cli": "^3.0.8"
  },
  "engines": {
    "node": "8.11.x",
    "npm": "5.6.x"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:partio-scout/reki.git"
  },
  "babel": {
    "presets": [
      "es2015-node4"
    ],
    "plugins": [
      "add-module-exports"
    ]
  },
  "private": true,
  "licence": "UNLICENCED"
}
