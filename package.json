{
  "name": "reki",
  "version": "0.0.0",
  "description": "Finnjamboree 2016 camp registry system",
  "main": "dist/server/server.js",
  "scripts": {
    "start": "node .",
    "build": "npm run build-server && npm run build-client",
    "build-server": "tsc --project src/server/",
    "build-client": "webpack -p --config webpack.config.js",
    "watch": "concurrently --k -i -n server,build-server,build-client,database -c black.bgGreen,black.bgBlue,black.bgYellow,black.bgRed \"npm:watch:server\" \"npm:watch:build-server\" \"npm:watch:build-client\" \"docker-compose up postgres\"",
    "watch:server": "nodemon --watch dist/server --delay 1",
    "watch:build-client": "webpack-dev-server --mode development --config webpack.config.js",
    "watch:build-server": "tsc -w --project src/server/",
    "set-password": "ts-node --project src/server/tsconfig.json scripts/set-password.js",
    "create-user": "ts-node --project src/server/tsconfig.json scripts/create-user.js",
    "delete-user": "ts-node --project src/server/tsconfig.json scripts/delete-user.js",
    "dev-login": "ts-node --project src/server/tsconfig.json scripts/dev-login.js",
    "test": "npm run lint && npm run test-functional && npm run test-integration && npm run test-e2e",
    "lint": "npm run eslint-check && npm run prettier-check",
    "eslint-check": "eslint --ext js --ext jsx --ext ts --ext tsx src test scripts webpack.config.js",
    "prettier-check": "prettier --check '**/*.{js,jsx,mjs,ts,tsx,json,md,css,less,scss}'",
    "test-functional": "TS_NODE_PROJECT=src/server/tsconfig.json NODE_ENV=test ENABLE_OFFLINE_LOGIN=true mocha --timeout 4000 --require ts-node/register test/functional --exit",
    "test-one": "TS_NODE_PROJECT=src/server/tsconfig.json NODE_ENV=test ENABLE_OFFLINE_LOGIN=true mocha --timeout 4000 --require ts-node/register --exit",
    "test-watch": "TS_NODE_PROJECT=src/server/tsconfig.json NODE_ENV=test ENABLE_OFFLINE_LOGIN=true mocha --watch --timeout 4000 --require ts-node/register --exit",
    "test-integration": "TS_NODE_PROJECT=src/server/tsconfig.json NODE_ENV=test mocha --timeout 4000 --require ts-node/register test/integration --exit",
    "pretest-e2e": "NODE_ENV=test npm run seed-database && npm run build",
    "test-e2e-mocha": "mocha --timeout 10000 --require ts-node/register test/e2e --exit",
    "test-e2e": "TS_NODE_PROJECT=src/server/tsconfig.json NODE_ENV=test PORT=3005 start-test 'mkdir -p logs && npm start > ./logs/server-test.log 2>&1' 3005 test-e2e-mocha",
    "seed-database": "ts-node --project src/server/tsconfig.json scripts/seed-database.js",
    "clear-temporary-tables": "ts-node --project src/server/tsconfig.json src/kuksa-integration/clear-temporary-tables.js",
    "fetch-data": "ts-node --project src/server/tsconfig.json src/kuksa-integration/fetch-data.js",
    "rebuild-tables": "ts-node --project src/server/tsconfig.json src/kuksa-integration/rebuild-tables.js",
    "fetch-from-kuksa": "npm run clear-temporary-tables && npm run fetch-data && npm run rebuild-tables",
    "fetch-from-mock-kuksa": "KUKSA_API_PROXY_URL=\"\" KUKSA_API_USERNAME=username KUKSA_API_PASSWORD=password KUKSA_API_EVENTID=c3ef7c48-1df4-4bf8-b1d0-d523d601a3e1 KUKSA_API_ENDPOINT=http://localhost:4001 npm run fetch-from-kuksa",
    "mock-kuksa": "ts-node --project src/server/tsconfig.json test/utils/kuksa-integration/mock/mock-kuksa.js",
    "dump-kuksa": "scripts/dump-kuksa-api.sh"
  },
  "dependencies": {
    "argon2": "^0.26.2",
    "body-parser": "^1.19.0",
    "connect-flash": "^0.1.1",
    "connect-redis": "^4.0.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-enforces-ssl": "^1.1.0",
    "express-session": "^1.17.1",
    "feather-route-matcher": "^3.1.0",
    "helmet": "^3.22.0",
    "http-proxy": "^1.18.0",
    "inquirer": "^7.1.0",
    "kuksa-event-api-client": "https://github.com/partio-scout/kuksa-event-api-client/releases/download/v4.0.0/kuksa-event-api-client.tar.gz",
    "lodash": "^4.17.15",
    "moment": "^2.25.1",
    "morgan": "^1.10.0",
    "passport": "^0.4.1",
    "passport-http": "^0.3.0",
    "passport-saml": "^1.3.3",
    "pg": "^8.0.3",
    "redis": "^3.0.2",
    "runtypes": "^4.2.0",
    "sequelize": "^5.21.7",
    "serve-favicon": "^2.5.0",
    "ts-node": "^8.9.1",
    "typescript": "^3.8.3"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "@hot-loader/react-dom": "^16.13.0",
    "@types/bluebird": "^3.5.30",
    "@types/feather-route-matcher": "^3.1.0",
    "@types/history": "^4.7.5",
    "@types/lodash": "^4.14.150",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.7",
    "@types/react-spinner": "^0.2.0",
    "@types/validator": "^13.0.0",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "acorn": "^7.1.1",
    "babel-loader": "^8.1.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-datetime": "^1.6.0",
    "concurrently": "^5.2.0",
    "core-js": "^3.6.5",
    "css-loader": "^3.5.3",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^4.0.0",
    "file-loader": "^6.0.0",
    "history": "^4.10.1",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.2",
    "mocha": "^7.1.2",
    "nodemon": "^2.0.3",
    "prettier": "^2.0.5",
    "puppeteer": "^3.0.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-hot-loader": "^4.12.21",
    "react-spinner": "^0.2.6",
    "start-server-and-test": "^1.11.0",
    "style-loader": "^1.2.1",
    "supertest": "^4.0.2",
    "ts-loader": "^7.0.2",
    "url-loader": "^4.1.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "engines": {
    "node": "10.16.3"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:partio-scout/reki.git"
  },
  "private": true,
  "licence": "UNLICENCED"
}
